<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5">
      <buildProviders>
        <add extension=".html" type="System.Web.Compilation.PageBuildProvider"/>
        <add extension=".shtml" type="System.Web.Compilation.PageBuildProvider"/>
        <!-- Allows for routing everything to ~/index.html -->
      </buildProviders>
    </compilation>
    <httpRuntime targetFramework="4.5" executionTimeout="30"/>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IDeposit"/>
        <binding name="BasicHttpBinding_IWithdrawal"/>
        <binding name="BasicHttpBinding_IFundTransfer"/>
        <binding name="BasicHttpBinding_IMember"/>
        <binding name="BasicHttpBinding_IMS1"/>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://paymentservices.aqzbo.tw/Deposit.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IDeposit" contract="svcPayDeposit.IDeposit" name="BasicHttpBinding_IDeposit"/>
      <endpoint address="http://paymentservices.aqzbo.tw/Withdrawal.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWithdrawal" contract="svcPayWithdraw.IWithdrawal" name="BasicHttpBinding_IWithdrawal"/>
      <endpoint address="http://paymentservices.aqzbo.tw/FundTransfer.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IFundTransfer" contract="svcPayTransfer.IFundTransfer" name="BasicHttpBinding_IFundTransfer"/>
      <endpoint address="http://paymentservices.aqzbo.tw/Member.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMember" contract="svcPayMember.IMember" name="BasicHttpBinding_IMember"/>
      <endpoint address="http://paymentservices.aqzbo.tw/MS1.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMS1" contract="svcPayMS.IMS1" name="BasicHttpBinding_IMS1"/>
    </client>
  </system.serviceModel>
  <appSettings>
    <add key="OperatorId" value="1"/>
    <add key="OperatorCode" value="W88"/>
    <add key="wsMemberCore.memberWS" value="http://ws.ms1dev.com/memberWS.asmx"/>
    <add key="wsIP2Loc.service" value="http://locationservices.ms1dev.com/service.asmx"/>
    <add key="wsMemberCoreMobile.mws_member" value="http://localhost:56714/mws_member.asmx"/>

    <add key="MS1Key" value="M41n$YsT3mVone"/>

  </appSettings>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
      <remove name="UrlRoutingModule"/>
      <add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    </modules>
    <handlers>
      <add name="UrlRoutingHandler" preCondition="integratedMode" verb="*" path="UrlRouting.axd" type="System.Web.HttpForbiddenHandler, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    </handlers>
    <staticContent>
      <mimeMap fileExtension=".appcache" mimeType="text/cache-manifest"/>
      <mimeMap fileExtension=".json" mimeType="application/json"/>
    </staticContent>
  </system.webServer>
</configuration>